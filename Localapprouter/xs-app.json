{
  "authenticationMethod": "route",
  "routes": [
  {
    "source": "^/posalp/(.*)$",
    "target": "$1",
    "localDir": "../app/pos_alp/webapp",
    "authenticationType": "xsuaa",
    "csrfProtection": false
  },
  {
    "source": "^/posalp$",
    "target": "/index.html",
    "localDir": "../app/pos_alp/webapp",
    "authenticationType": "xsuaa",
    "csrfProtection": false
  },
    {
      "source": "^/base/(.*)$",
      "target": "/base/$1",
      "authenticationType": "xsuaa",
      "destination": "srv-api",
      "csrfProtection": false
    },
    {
      "source": "^/ds/(.*)$",
      "target": "$1",
      "authenticationType": "xsuaa",
      "destination": "datasphere"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    }
  ]
}